{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "jsx": "preserve",
    "lib": ["ESNext"],
    "module": "esnext",
    "target": "esnext",
    "skipLibCheck": true,
    "types": ["bun-types"],
    "esModuleInterop": true,
    "downlevelIteration": true,
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  }
}
